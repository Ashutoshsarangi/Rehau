{
  "name": "rehau-ui-core",
  "version": "0.0.0",
  "description": "This package contains a common set of reusable ui components to be used as reusable component across applications.",
  "author": "Rehau",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/interface.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/rehau-ui-core/rehau-ui-core.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "repository": {
    "type": "git"
  },
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "test": "stencil test --spec --e2e"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "tslint --fix",
      "git add ."
    ],
    "*.{md,json}": [
      "prettier",
      "git add ."
    ]
  },
  "devDependencies": {
    "@stencil/core": "1.4.0",
    "@stencil/postcss": "1.0.1",
    "@stencil/router": "1.0.1",
    "@stencil/sass": "1.0.1",
    "@types/autoprefixer": "^9.5.0",
    "@types/jest": "24.0.25",
    "@types/puppeteer": "1.19.1",
    "jest": "24.8.0",
    "jest-cli": "24.8.0",
    "lint-staged": "^9.2.5",
    "pre-commit": "^1.2.2",
    "puppeteer": "1.19.0",
    "tslint": "^5.10.0",
    "tslint-react": "^3.6.0"
  },
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^9.6.1",
    "flexboxgrid-sass": "^8.0.5",
    "glob": "^7.1.4",
    "glob-promise": "^3.4.0",
    "hammerjs": "^2.0.8",
    "inputmask": "^4.0.8",
    "ionicons": "^4.6.3",
    "marked": "^0.7.0",
    "mermaid": "^8.1.0",
    "moment": "^2.24.0",
    "rollup-plugin-alias": "^1.5.2",
    "rxjs": "6.0.0",
    "tippy.js": "^5.1.0"
  }
}
